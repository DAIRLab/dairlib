group "director" {
    cmd "drake-director" {
        exec = "bazel-bin/director/drake-director";
        host = "localhost";
    }
}
cmd "impedance controller" {
    exec = "bazel-bin/examples/franka_trajectory_following/lcm_control_impedence";
    host = "localhost";
}
cmd "franka" {
    exec = "bazel-bin/examples/franka_trajectory_following/simulate_franka_lcm";
    host = "localhost";
}
cmd "C3" {
    exec = "bazel-bin/examples/franka_trajectory_following/lcm_control_demo";
    host = "localhost";
}

