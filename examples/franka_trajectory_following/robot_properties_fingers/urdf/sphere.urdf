<?xml version="1.0" ?>
<robot name="sphere_v2">
    <link name="capsule_1">
        <contact>
            <rolling_friction value="0.5"/>
            <spinning_friction value="0.5"/>
        </contact>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="0.1"/>
            <inertia ixx="0.000136" ixy="0" ixz="0" iyy="0.000136" iyz="0" izz="1.1e-05"/>
        </inertial>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <capsule length="0.125" radius="0.015"/> 
            </geometry>
        </collision>
        <drake:proximity_properties>
                <!-- POINT CONTACT -->
                <drake:mu_static value="1"/>
                <drake:mu_dynamic value="1"/>

                <!-- HYDROELASTIC CONTACT -->
                <!-- <drake:compliant_hydroelastic/> -->
                <!-- <drake:mesh_resolution_hint value="0.02"/> -->
                <!-- <drake:hydroelastic_modulus value="1e7"/> -->
                <!-- <drake:hunt_crossley_dissipation value ="1.25"/> -->
        </drake:proximity_properties>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <capsule length="0.125" radius="0.015"/> 
            </geometry>
        </visual>
    </link>

    <link name="capsule_2">
        <contact>
            <rolling_friction value="0.5"/>
            <spinning_friction value="0.5"/>
        </contact>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="0.1"/>
            <inertia ixx="0.000136" ixy="0" ixz="0" iyy="0.000136" iyz="0" izz="1.1e-05"/>
        </inertial>
        <drake:proximity_properties>
                <!-- POINT CONTACT -->
                <drake:mu_static value="1"/>
                <drake:mu_dynamic value="1"/>

                <!-- HYDROELASTIC CONTACT -->
                <!-- <drake:compliant_hydroelastic/> -->
                <!-- <drake:mesh_resolution_hint value="0.02"/> -->
                <!-- <drake:hydroelastic_modulus value="1e7"/> -->
                <!-- <drake:hunt_crossley_dissipation value ="1.25"/> -->
        </drake:proximity_properties>
        <collision>
            <origin rpy="1.5708 0 0" xyz="0 0 0"/> <!-- Rotate by 90 degrees around the x-axis -->
            <geometry>
                <capsule length="0.125" radius="0.015"/> 
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5708 0 0" xyz="0 0 0"/> <!-- Rotate by 90 degrees around the x-axis -->
            <geometry>
                <capsule length="0.125" radius="0.015"/> 
            </geometry>
        </visual>
    </link>

    <link name="capsule_3">
        <!-- Third capsule (perpendicular to the first two capsules) -->
        <contact>
            <rolling_friction value="0.5"/>
            <spinning_friction value="0.5"/>
        </contact>
        <drake:proximity_properties>
                <!-- POINT CONTACT -->
                <drake:mu_static value="1"/>
                <drake:mu_dynamic value="1"/>

                <!-- HYDROELASTIC CONTACT -->
                <!-- <drake:compliant_hydroelastic/> -->
                <!-- <drake:mesh_resolution_hint value="0.02"/> -->
                <!-- <drake:hydroelastic_modulus value="1e7"/> -->
                <!-- <drake:hunt_crossley_dissipation value ="1.25"/> -->
        </drake:proximity_properties>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="0.1"/>
            <inertia ixx="0.000136" ixy="0" ixz="0" iyy="0.000136" iyz="0" izz="1.1e-05"/>
        </inertial>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <capsule length="0.12" radius="0.015"/> 
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <capsule length="0.12" radius="0.015"/> 
            </geometry>
        </visual>
    </link>

    <!-- Small contact sphere for the first capsule -->
    <link name="contact_sphere_1_end1">
        <inertial>
            <mass value="0.0001"/> <!-- Mass of the small sphere -->
            <inertia ixx="1e-08" ixy="0" ixz="0" iyy="1e-08" iyz="0" izz="1e-08"/> <!-- Inertia tensor of the small sphere -->
        </inertial>
        
        <visual>
            <origin rpy="0 0 0" xyz="0 0.065 0"/> <!-- Origin at the center of the sphere -->
            <geometry>
                <sphere radius="0.005"/> <!-- Radius of the small sphere -->
            </geometry>
            <material>
                <color rgba="1 0 0 1"/> <!-- Red color for visualization -->
            </material>
        </visual>
    </link>

    <link name="contact_sphere_1_end2">
        <inertial>
            <mass value="0.0001"/> <!-- Mass of the small sphere -->
            <inertia ixx="1e-08" ixy="0" ixz="0" iyy="1e-08" iyz="0" izz="1e-08"/> <!-- Inertia tensor of the small sphere -->
        </inertial>
        
        <visual>
            <origin rpy="0 0 0" xyz="0 -0.065 0"/> <!-- Origin at the center of the sphere -->
            <geometry>
                <sphere radius="0.005"/> <!-- Radius of the small sphere -->
            </geometry>
            <material>
                <color rgba="1 0 0 1"/> <!-- Red color for visualization -->
            </material>
        </visual>
    </link>

    <link name="contact_sphere_2_end1">
        <inertial>
            <mass value="0.0001"/> <!-- Mass of the small sphere -->
            <inertia ixx="1e-08" ixy="0" ixz="0" iyy="1e-08" iyz="0" izz="1e-08"/> <!-- Inertia tensor of the small sphere -->
        </inertial>
        
        <visual>
            <origin rpy="0 0 0" xyz="0 0.065 0"/> <!-- Origin at the center of the sphere -->
            <geometry>
                <sphere radius="0.005"/> <!-- Radius of the small sphere -->
            </geometry>
            <material>
                <color rgba="1 0 0 1"/> <!-- Red color for visualization -->
            </material>
        </visual>
    </link>

    <link name="contact_sphere_2_end2">
        <inertial>
            <mass value="0.0001"/> <!-- Mass of the small sphere -->
            <inertia ixx="1e-08" ixy="0" ixz="0" iyy="1e-08" iyz="0" izz="1e-08"/> <!-- Inertia tensor of the small sphere -->
        </inertial>
        
        <visual>
            <origin rpy="0 0 0" xyz="0 -0.065 0"/> <!-- Origin at the center of the sphere -->
            <geometry>
                <sphere radius="0.005"/> <!-- Radius of the small sphere -->
            </geometry>
            <material>
                <color rgba="1 0 0 1"/> <!-- Red color for visualization -->
            </material>
        </visual>
    </link>

    <link name="contact_sphere_3_end1">
        <inertial>
            <mass value="0.0001"/> <!-- Mass of the small sphere -->
            <inertia ixx="1e-08" ixy="0" ixz="0" iyy="1e-08" iyz="0" izz="1e-08"/> <!-- Inertia tensor of the small sphere -->
        </inertial>
        
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.065"/> <!-- Origin at the center of the sphere -->
            <geometry>
                <sphere radius="0.005"/> <!-- Radius of the small sphere -->
            </geometry>
            <material>
                <color rgba="1 0 0 1"/> <!-- Red color for visualization -->
            </material>
        </visual>
    </link>

    <link name="contact_sphere_3_end2">
         <inertial>
            <mass value="0.0001"/> <!-- Mass of the small sphere -->
            <inertia ixx="1e-08" ixy="0" ixz="0" iyy="1e-08" iyz="0" izz="1e-08"/> <!-- Inertia tensor of the small sphere -->
        </inertial>
       
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -0.065"/> <!-- Origin at the center of the sphere -->
            <geometry>
                <sphere radius="0.005"/> <!-- Radius of the small sphere -->
            </geometry>
            <material>
                <color rgba="1 0 0 1"/> <!-- Red color for visualization -->
            </material>
        </visual>
    </link>


    <!-- Joints to connect the capsules-->
    <joint name="attach_joint" type="fixed">
        <parent link="capsule_1"/>
        <child link="capsule_2"/>
        <origin rpy="0 1.5708 0" xyz="0 0 0"/> <!-- Rotate by 90 degrees around the y-axis -->
    </joint>

    <joint name="attach_joint2" type="fixed">
        <parent link="capsule_2"/>
        <child link="capsule_3"/>
        <origin rpy="0 0 1.5708" xyz="0 0 0"/> <!-- Rotate by 90 degrees around the z-axis -->
    </joint>

    <!-- Joint to attach the first small contact sphere to the first capsule, at one end -->
    <joint name="attach_sphere_joint_1_end1" type="fixed">
        <parent link="capsule_2"/>
        <child link="contact_sphere_1_end1"/>
        <origin rpy="0 0 0" xyz="0 0.01325 0"/> <!-- Offset along the z-axis to attach at one end of the capsule -->
    </joint>

    <!-- Joint to attach the first small contact sphere to the first capsule, at the other end -->
    <joint name="attach_sphere_joint_1_end2" type="fixed">
        <parent link="capsule_2"/>
        <child link="contact_sphere_1_end2"/>
        <origin rpy="0 0 0" xyz="0 -0.01325 0"/> <!-- Offset along the z-axis (opposite direction) to attach at the other end of the capsule -->
    </joint>

    <!-- Joint to attach the second small contact sphere to the second capsule, at one end -->
    <joint name="attach_sphere_joint_2_end1" type="fixed">
        <parent link="capsule_3"/>
        <child link="contact_sphere_2_end1"/>
        <origin rpy="0 0 0" xyz="0 0.01325 0"/> <!-- Offset along the z-axis to attach at one end of the capsule -->
    </joint>

    <!-- Joint to attach the second small contact sphere to the second capsule, at the other end -->
    <joint name="attach_sphere_joint_2_end2" type="fixed">
        <parent link="capsule_3"/>
        <child link="contact_sphere_2_end2"/>
        <origin rpy="0 0 0" xyz="0 -0.01325 0"/> <!-- Offset along the z-axis (opposite direction) to attach at the other end of the capsule -->
    </joint>

    <!-- Joint to attach the third small contact sphere to the third capsule, at one end -->
    <joint name="attach_sphere_joint_3_end1" type="fixed">
        <parent link="capsule_3"/>
        <child link="contact_sphere_3_end1"/>
        <origin rpy="0 0 0" xyz="0 0 0.01325"/> <!-- Offset along the z-axis to attach at one end of the capsule -->
    </joint>

    <!-- Joint to attach the third small contact sphere to the third capsule, at the other end -->
    <joint name="attach_sphere_joint_3_end2" type="fixed">
        <parent link="capsule_3"/>
        <child link="contact_sphere_3_end2"/>
        <origin rpy="0 0 0" xyz="0 0 -0.01325"/> <!-- Offset along the z-axis (opposite direction) to attach at the other end of the capsule -->
    </joint>
</robot>
