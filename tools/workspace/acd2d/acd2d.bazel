# -*- python -*-

package(default_visibility = ["//visibility:public"])

# Prebuilt C++ and Python ROS libraries including local, unpublished packages

cc_library(
    name='fist',
    hdrs=glob(['libs/FIST/*.h']),
    strip_include_prefix='libs/FIST',
    deps=[]
)

cc_library(
    name='acd2d',
    srcs=glob(['src/**/*.cpp']),
    hdrs=glob(['src/**/*.h']),
    strip_include_prefix='src',
    deps=['fist']
)

